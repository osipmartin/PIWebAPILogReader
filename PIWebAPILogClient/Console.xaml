<Window x:Class="PIWebAPILogClient.Console"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PIWebAPILogClient"
        mc:Ignorable="d"
        Title="Console" Height="300" Width="500">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="500*"/>
        </Grid.ColumnDefinitions>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="216" Margin="10,10,0,0" VerticalAlignment="Top" Width="472" RenderTransformOrigin="-0.699,-0.214" FontFamily="Consolas"/>
        <Button x:Name="Close" Content="Close" HorizontalAlignment="Left" Margin="165,238,0,0" VerticalAlignment="Top" Width="170" Click="Close_Click"/>

    </Grid>
</Window>
